    "tailwindcss": "^4.0.0",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^7.0.4",
    "vitest": "^3.2.3",
    "vitest-browser-svelte": "^0.1.0"
  },
