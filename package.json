    "@playwright/test": "^1.49.1",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^24.0.15",
    "@vitest/browser": "^3.2.3",
    "bits-ui": "^2.8.11",
    "clsx": "^2.1.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "formsnap": "^2.0.1",
    "globals": "^16.0.0",
